{
	"name": "weightloss-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"cf-typegen": "wrangler types",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 execute weightloss-db --local --file=./drizzle/migrations/0000_initial_schema.sql",
		"db:migrate:remote": "wrangler d1 execute weightloss-db --remote --file=./drizzle/migrations/0000_initial_schema.sql",
		"db:seed": "node scripts/seed-users.js",
		"db:seed:remote": "node scripts/seed-users.js --remote"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcryptjs": "^3.0.0",
		"drizzle-kit": "^0.31.6",
		"svelte": "^5.0.0",
		"vite": "^5.0.0",
		"wrangler": "^4.46.0"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"drizzle-orm": "^0.44.7"
	}
}
